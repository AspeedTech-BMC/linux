			#size-cells = <0>;
			reg = <0x0 0x30c20000 0x0 0x1000>;
			interrupts-extended = <&ltpi0_intc1_1 0>;
			i3c-scl-hz = <1000000>;
			clocks = <&ltpi0_syscon AST1700_CLK_GATE_I3C0CLK>;
