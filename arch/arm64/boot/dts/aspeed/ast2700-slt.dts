	pinctrl_rgmii0_driving: rgmii0-driving {
		pins = "C20", "C19", "A8", "R14", "A7", "P14",
		       "D20", "A6", "B6", "N14", "B7", "B8";
		drive-strength = <1>;
	};
};

&gpio0 {
	pinctrl-0 = <&pinctrl_i3c12_driving &pinctrl_i3c13_driving
		     &pinctrl_i3c14_driving &pinctrl_i3c15_driving>;
	pinctrl-names = "default";
