	fan-5 {
		tach-ch = /bits/ 8 <0x5>;
	};
	fan-6 {
		tach-ch = /bits/ 8 <0x6>;
	};
	fan-7 {
		tach-ch = /bits/ 8 <0x7>;
	};
};

