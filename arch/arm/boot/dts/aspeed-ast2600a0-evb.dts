};

&video {
	status = "okay";
	memory-region = <&video_engine_memory>;
};

&vhub {
	status = "okay";
	pinctrl-names = "default";
#if 1
	pinctrl-0 = <&pinctrl_usb2ad_default>;
#else
	pinctrl-0 = <&pinctrl_usb2adp_default>;
#endif
};

#if 0
&ehci0 {
	status = "okay";
};
#endif

&ehci1 {
	status = "okay";
};

&sdc {
	status = "okay";
	timing-phase = <0xc6ffff>;
};

&emmc_controller {
	status = "okay";
	timing-phase = <0x700FF>;
};

&emmc {
	status = "okay";

	non-removable;
	max-frequency = <100000000>;
	/* AST2600 A0 only support PIO mode */
	sdhci-caps-mask = <0x0 0x580000>;
#if 1
	bus-width = <4>;
#else
	bus-width = <8>;
	pinctrl-0 = <&pinctrl_emmc_default 
			&pinctrl_emmcdat4_default 
			&pinctrl_emmcdat5_default
			&pinctrl_emmcdat6_default
			&pinctrl_emmcdat7_default>;
#endif
};

&sdhci0 {
	status = "okay";

	bus-width = <4>;
	max-frequency = <100000000>;

	/* SDR104/DDR50/SDR50 bits in CAPA2 is not supported */
	sdhci-caps-mask = <0x7 0x0>;
	sdhci,wp-inverted;
};

&sdhci1 {
	status = "okay";

	bus-width = <4>;
	max-frequency = <100000000>;
	sdhci-caps-mask = <0x7 0x0>;
	sdhci,wp-inverted;
};

&peci0 {
	status = "okay";
};

&pcie {
	status = "okay";

	interrupts = <GIC_SPI 29 IRQ_TYPE_EDGE_RISING>;

	pcie_intc0: legacy-interrupt-controller {
		interrupts = <GIC_SPI 29 IRQ_TYPE_EDGE_RISING>;
	};
};

&h2x {
	status = "okay";
};

&sgpiom0 {
        status = "okay";
};

&adc0 {
	trim_locate = <0x5D0 0 0xf>;
	ref_voltage = <1200>;
	status = "okay";

	pinctrl-0 = <&pinctrl_adc0_default &pinctrl_adc1_default
		&pinctrl_adc2_default &pinctrl_adc3_default
		&pinctrl_adc4_default &pinctrl_adc5_default
		&pinctrl_adc6_default &pinctrl_adc7_default>;
};

&adc1 {
	trim_locate = <0x5D0 4 0xf>;
	ref_voltage = <1200>;
	status = "okay";

	pinctrl-0 = <&pinctrl_adc8_default &pinctrl_adc9_default
		&pinctrl_adc10_default &pinctrl_adc11_default
		&pinctrl_adc12_default &pinctrl_adc13_default
		&pinctrl_adc14_default &pinctrl_adc15_default>;
};

&jtag1 {
	status = "okay";
};
